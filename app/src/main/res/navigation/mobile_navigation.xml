<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/nav_home">

    <fragment
        android:id="@+id/nav_home"
        android:name="com.example.todotrial.ui.home.HomeFragment"
        android:label="@string/menu_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/nav_gallery"
        android:name="com.example.todotrial.ui.gallery.GalleryFragment"
        android:label="@string/menu_gallery"
        tools:layout="@layout/fragment_gallery" />

    <fragment
        android:id="@+id/nav_slideshow"
        android:name="com.example.todotrial.ui.slideshow.SlideshowFragment"
        android:label="@string/menu_slideshow"
        tools:layout="@layout/fragment_slideshow" />
    <activity
        android:id="@+id/nav_exercise"
        android:name="com.example.todotrial.exercise"
        android:label="@string/menu_slideshow"
        tools:layout="@layout/activity_exercise"
        />
    <activity
        android:id="@+id/todo"
        android:name="com.example.todotrial.todo"
        android:label="@string/menu_slideshow"
        tools:layout="@layout/activity_todo"
        />
    <activity
        android:id="@+id/sports"
        android:name="com.example.todotrial.sports"
        android:label="@string/menu_slideshow"
        tools:layout="@layout/activity_sports"
        />
</navigation>